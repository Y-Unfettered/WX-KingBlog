.Typeface_Arial {
  font-family: Arial, Helvetica, sans-serif;
}
.container {
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;  
  margin: 20rpx 0 40rpx 0rpx;
  padding-bottom: 5rpx;
}

.container_nav {
  margin: 10rpx 0 20rpx 10rpx;
}

.container_image {
  vertical-align: middle;
  height: 60rpx;
  width: 60rpx;
}

.container text {
  font-size: 26rpx;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
  vertical-align: middle;
}

.container_text1 {
  font-size: 34rpx;
  font-weight: 600;
  color: #333;
  margin-bottom: 10px;
  margin-left: 10px;
}

.container_image1 {
  width: 100%;
  height: 340rpx;
  margin: 0 auto;
  padding-bottom: 20rpx;
}

.container_text2 {
  color: #666;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  letter-spacing: 2rpx;
  line-height: 40rpx;
}

.container_image2 {
  width: 16px;
  height: 16px;
  margin-left: 5rpx;
}

.container_font {
  font-size: 24rpx;
  color: #666;
  vertical-align: middle;
  margin-right: 20px;
}
